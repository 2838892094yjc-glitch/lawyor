# Copy to your shell env before starting Kimi bridge.
# Example:
# export KIMI_API_KEY="sk-..."
# export KIMI_MODEL="moonshot-v1-32k"
# npm run ai:kimi

KIMI_API_KEY=
# Optional alias (either one works):
# MOONSHOT_API_KEY=

# Legacy direct mode defaults
KIMI_MODEL=moonshot-v1-32k
KIMI_TEMPERATURE=0
KIMI_REQUEST_TIMEOUT_MS=30000
KIMI_MAX_TOKENS=1200

# Two-stage pipeline (recommended)
KIMI_PIPELINE_ENABLED=1
KIMI_PIPELINE_FALLBACK_DIRECT=1
KIMI_SEMANTIC_MODEL=kimi-k2.5
KIMI_SEMANTIC_TEMPERATURE=1
KIMI_SEMANTIC_MAX_TOKENS=1800
KIMI_SEMANTIC_TIMEOUT_MS=70000
KIMI_STRUCT_MODEL=moonshot-v1-32k
KIMI_STRUCT_TEMPERATURE=0
KIMI_STRUCT_MAX_TOKENS=1200
KIMI_STRUCT_TIMEOUT_MS=30000
KIMI_NETWORK_RETRIES=1

AI_BRIDGE_PORT=8765

# Optional: switch to legacy skill prompt
# SKILL_PROMPT_PATH=/Users/you/path/to/contract-variable-skill/SKILL.md
